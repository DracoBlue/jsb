{
  "name": "node-jsb",
  "description": "A very extendable Toolkit to inject Javascript Behaviour into rendered HTML without Inline Javascript.",
  "typings": "./jsb.d.ts",
  "main": "./dist/jsb.js",
  "module": "./dist/jsb.es.js",
  "files": [
    "dist"
  ],
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "test": "mocha",
    "build": "npm run build:ts && npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "rollup -c",
    "build:ts": "tsc --declaration --allowJs --emitDeclarationOnly jsb.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DracoBlue/jsb.git"
  },
  "keywords": [
    "ajax",
    "data-binding",
    "domready",
    "dynamic"
  ],
  "author": "DracoBlue <JanS@DracoBlue.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DracoBlue/jsb/issues"
  },
  "homepage": "https://github.com/DracoBlue/jsb#readme",
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@rollup/plugin-babel": "6.0.2",
    "mocha": "10.1.0",
    "rimraf": "3.0.2",
    "rollup": "3.2.3",
    "typescript": "4.8.4"
  },
  "browserslist": [
    "> .5% and last 2 versions",
    "not ie <= 11",
    "not dead"
  ]
}
